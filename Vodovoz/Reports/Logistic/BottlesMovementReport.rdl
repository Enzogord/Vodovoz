	fines on fines.route_list_id = route_lists.id
left join
	(select
		route_list_addresses.route_list_id as route_list_id,
		sum(ifnull(orders.returned_tare,0)) as bottles_to_return,
		sum(route_list_addresses.bottles_returned) as bottles_returned,
		sum(o_items.count) as count,
		sum(o_items.actual_count) as actual_count
	from
		route_list_addresses
	left join
		orders on orders.id = route_list_addresses.order_id
	left join 
		(select
			order_items.order_id,
			sum(order_items.count) as count,
			sum(order_items.actual_count) as actual_count
		from
			order_items
		left join
			nomenclature on nomenclature.id = order_items.nomenclature_id
		where
			nomenclature.category = 'water'
		group by
			order_items.order_id
		) as o_items on o_items.order_id = orders.id
	group by
		route_list_addresses.route_list_id) as rla on rla.route_list_id = route_lists.id
left join
	(select
		store_car_unload_documents.route_list_id as route_list_id,
		sum(ifnull(warehouse_movement_operations.amount, 0)) as amount
	from
		store_car_unload_documents
	left join
		store_car_unload_document_items on store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
	left join
		warehouse_movement_operations on warehouse_movement_operations.id = store_car_unload_document_items.warehouse_movement_operation_id
	left join
		nomenclature on nomenclature.id = warehouse_movement_operations.nomenclature_id
	where
		nomenclature.category = 'water'
	group by
		store_car_unload_documents.route_list_id) as returned_full on returned_full.route_list_id = route_lists.id
left join
	(select
		store_car_unload_documents.route_list_id as route_list_id,
		sum(ifnull(warehouse_movement_operations.amount, 0)) as amount
	from
		store_car_unload_documents
	left join
		store_car_unload_document_items on store_car_unload_document_items.car_unload_document_id = store_car_unload_documents.id
	left join
		warehouse_movement_operations on warehouse_movement_operations.id = store_car_unload_document_items.warehouse_movement_operation_id
	left join
		nomenclature on nomenclature.id = warehouse_movement_operations.nomenclature_id
	where
		nomenclature.category = 'bottle'
	group by
		store_car_unload_documents.route_list_id) as returned_empty on returned_empty.route_list_id = route_lists.id
where
	route_lists.date &gt;= @start_date
	and route_lists.date &lt;= @end_date</CommandText>
